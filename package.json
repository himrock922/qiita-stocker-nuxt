{
  "name": "qiita-stocker-nuxt",
  "version": "1.0.0",
  "description": "Mindexer Nuxt.js project",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
